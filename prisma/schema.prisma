
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}


model User {
  id          Int     @id @default(autoincrement())
  email       String  @unique
  password    String
  firstName   String?
  lastName    String?
  phoneNumber String?
  progress    Float   @default(0.0) // Progress as percentage (0.0 to 100.0)
}
